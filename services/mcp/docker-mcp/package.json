{
  "name": "docker-mcp-server",
  "version": "0.3.0",
  "description": "Docker MCP Server with advanced container management and dual-version Docker Compose support",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "start:http": "node dist/index.js --transport http",
    "start:stdio": "node dist/index.js --transport stdio",
    "build": "tsc",
    "dev": "tsc -w",
    "prepare": "npm run build",
    "test": "node test.js",
    "install-config": "node install.js"
  },
  "keywords": [
    "docker",
    "docker-compose",
    "compose",
    "mcp",
    "server",
    "claude",
    "cline"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "@types/yargs": "^17.0.33",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
